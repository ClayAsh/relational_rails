<a href="/projects">Projects</a>
<a href='/volunteers'>Volunteers</a>
    <h1>Volunteers for this Project</h1>
    <h2><%= @project.name %></h2>
    <% @volunteers.each_with_index do |volunteer, index|  %>
    <div id="volunteer-<%= index %>"> 
        <h3><%= volunteer.name %></h3> 
        <a href="/volunteers/<%= volunteer.id %>/edit">Update Volunteer</a>
        <p>Hours Available: <%= volunteer.hours_available %></p>
        <p>Is this Volunteer Registered? <%= volunteer.registered %></p>
        <p>Volunteer Start Date:<%= volunteer.created_at %></p>
        <p>Information Updated:<%= volunteer.updated_at %></p>
     </div>
    <% end %>
    
    <li><a href="/projects/<%= @project.id %>/volunteers/new">Sign Up to Volunteer for this Project!</a></li>
    <li><a href="/projects/<%= @project.id %>/volunteers?sort=asc">Sort Volunteers</a></li>