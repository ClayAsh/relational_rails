<%= link_to "Create a New Project", "/projects/new"%>

<h1>All Projects</h1>
<% @projects.each do |project| %>
  <div id="<%= project.id %>"> 
    <h3><%= link_to "#{project.name}", "/projects/#{project.id}"%></h3>
    <%= link_to 'Update Project', "/projects/#{project.id}/edit" %></br>
    <%= link_to 'Delete Project', "/projects/#{project.id}", method: :delete %>
    <p>Project Start Date:<%= project.created_at %></p>
  </div>
<% end %>

